    "# Pre-processing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 53,
   "id": "923ff817",
   "metadata": {},
   "outputs": [],
   "source": [
    "def fill_missing(column_list, strategy, df):\n",
    "  '''\n",
    "  Takes column_list, strategy and dataframe as input and fills na values\n",
    "  with the specified strategy and returns the datafram\n",
    "  '''\n",
    "  if strategy==\"mode\":\n",
    "    for column in column_list:\n",
    "      df[column].fillna(df[column].mode()[0],inplace=True)\n",
    "\n",
    "  if strategy ==\"median\":\n",
    "    for column in column_list:\n",
    "      df[column].fillna(df[column].median(), inplace=True)\n",
    "\n",
    "  #returning the modified df\n",
    "  return df\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 54,
   "id": "e62fd5fe",
   "metadata": {},
   "outputs": [
    {
     "data": {
