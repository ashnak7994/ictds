   "source": [
    "def make_submission_file(y_pred, submission_id,id=loan_id):\n",
    "  '''\n",
    "  Make a csv file from the predictions\n",
    "  '''\n",
    "  df = pd.DataFrame({'Loan_ID':id,'Loan_Status':y_pred})\n",
    "  df.Loan_Status=df.Loan_Status.map({0:'N',1:'Y'})\n",
    "  df.to_csv(f\"Submission_{submission_id}.csv\", index=False)\n",
    "  return df\n",
    "     "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 68,
   "id": "2ac7013b",
   "metadata": {},
   "outputs": [
